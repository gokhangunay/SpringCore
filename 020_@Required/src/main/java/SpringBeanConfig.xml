<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.2.xsd">
	
	<context:annotation-config/> <!-- beanlerim icerisinde annotation varsa sen bunu dikkate al canim :) -->
	
	<bean id="beanStudent" class="com.gokhangunay.Student">
		<property name="name" value="Gökhan"></property>
		<property name="surName" value="Günay"></property>
		<!-- <property name="age" value="1989"></property> -->
	</bean>

</beans>
